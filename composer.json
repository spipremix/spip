            "php": "5.4.45"
        }
    },
    "extra": {
        "spip": {
            "template": "spip/dist",
            "extensions": [
                "spip/mots",
                "spip/organiseur",
                "spip/images",
                "spip/archiviste",
                "spip/breves",
                "spip/forum",
                "spip/medias",
                "spip/sites",
                "spip/aide",
                "spip/compagnon",
                "spip/compresseur",
                "spip/dump",
                "spip/jqueryui",
                "spip/mediabox",
                "spip/petitions",
                "spip/plan",
                "spip/porte-plume",
                "spip/revisions",
                "spip/safehtml",
                "spip/squelettes-par-rubrique",
                "spip/stats",
                "spip/svp",
                "spip/tw",
                "spip/urls",
                "spip/vertebres"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Spip\\Composer\\Security::postInstall"
        ],
        "post-update-cmd": [
            "Spip\\Composer\\Security::postUpdate"
        ]
    }
