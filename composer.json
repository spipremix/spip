{
    "name": "spip/spip",
    "type": "project",
    "repositories": [{
        "type": "composer",
        "url": "https://composer-spip.lerebooteux.fr"
    }],
    "require": {
        "php": "^5.4|^7",
        "spip/composer-installer": "^0.2",
        "spip/cms": "dev-master",
        "spip/mots": "dev-master",
        "spip/organiseur": "dev-master",
        "spip/images": "dev-master",
        "spip/security": "*",
        "spip/dist": "dev-master"
    },
    "license": "GPL-3.0-or-later",
    "authors": [{
        "name": "Arnaud Martin",
        "email": "arno@rezo.net"
    },{
        "name": "Antoine Pitrou",
        "email": "antoine@pitrou.net"
    },{
        "name": "Philippe Rivi√®re",
        "email": "fil@rezo.net"
    },{
        "name": "Emmanuel Saint-James",
        "email": "esj@rezo.net"
    }],
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "5.4.45"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.3.x-dev"
        },
        "spip": {
            "template": "spip/dist",
            "extensions": [
                "spip/mots",
                "spip/organiseur",
                "spip/images"
            ]
        }
    }
}
