                "spip/jqueryui",
                "spip/mediabox",
                "spip/petitions",
                "spip/plan",
                "spip/porte-plume",
                "spip/revisions",
                "spip/safehtml",
                "spip/squelettes-par-rubrique",
                "spip/stats",
                "spip/svp",
                "spip/tw",
                "spip/urls",
                "spip/vertebres"
